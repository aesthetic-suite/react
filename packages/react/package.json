{
  "name": "@aesthetic/react",
  "version": "0.6.13",
  "description": "React hooks, composers, and more for styling components with Aesthetic.",
  "keywords": [
    "aesthetic",
    "react",
    "styles",
    "hook",
    "hoc",
    "composer"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:aesthetic-suite/react.git",
    "directory": "packages/react"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "type": "module",
  "main": "./esm/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "server.d.ts",
    "server.js",
    "src/**/*.{ts,tsx,json}"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aesthetic/core": "^0.9.6",
    "@aesthetic/core-react": "^0.1.12",
    "@aesthetic/style": "^0.8.1",
    "@aesthetic/utils": "^0.7.0"
  },
  "peerDependencies": {
    "react": "^16.6.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rut-dom": "^2.0.0"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "server": "src/server.tsx"
    },
    "platform": "browser"
  },
  "tsconfig": {
    "compilerOptions": {
      "types": [
        "node",
        "react"
      ]
    }
  }
}
